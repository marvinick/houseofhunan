<br/>
<div class="row">
  <center><%= link_to "New Menu", new_menu_path %></center>
  <% @menus.each do |menu| %>
    <div class="col-md-4">
        <h4 class="page-header text-center"><%= link_to menu.name, menu_path(menu) %></h4>
        <center><%= link_to (image_tag menu.attachment_url(:thumb).to_s), menu %></center>
    </div>
  <% end %>
</div>
